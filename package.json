{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "A robust Discord bot template with modern architecture and best practices",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "deploy": "node src/scripts/deploy-commands.js",
    "deploy:global": "node src/scripts/deploy-commands.js --global",
    "deploy:dev": "node src/scripts/deploy-commands.js --dev",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "jest"
  },
  "keywords": [
    "discord",
    "bot",
    "template",
    "discord.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
